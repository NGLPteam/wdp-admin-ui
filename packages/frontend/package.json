    "lint:css": "stylelint --config .stylelintrc.js \"**/*.{ts,tsx}\"",
    "fix": "concurrently -c \"auto\" \"yarn:fix:*\"",
    "fix:js": "eslint --fix --cache -c .eslintrc.js \"**/*.{js,ts,tsx}\"",
    "types": "npx tsc --strict -w",
    "depcheck": "depcheck"
  },

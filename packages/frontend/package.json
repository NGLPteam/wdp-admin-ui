{
  "name": "@wdp/frontend",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "relay": "relay-compiler",
    "schema": "graphql-codegen",
    "storybook": "start-storybook -s ./public -p 6007",
    "build-storybook": "build-storybook -s ./public -o ./build/storybook",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --cache  -c .eslintrc.js \"**/*.{js,ts,tsx}\"",
    "lint:css": "stylelint --config .stylelintrc.js \"**/*.{ts,tsx}\"",
    "fix": "npm-run-all fix:*",
    "fix:js": "eslint --cache -c .eslintrc.js \"**/*.{js,ts,tsx}\" --fix",
    "types": "npx tsc --strict -w"
  },
  "dependencies": {
    "@castiron/style-mixins": "^1.0.6",
    "@react-keycloak/ssr": "^3.3.0",
    "@react-keycloak/web": "^3.4.0",
    "@wdp/theme": "1.0.0",
    "cookie": "^0.4.1",
    "date-fns": "^2.25.0",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "keycloak-js": "^12.0.4",
    "lodash": "^4.17.21",
    "next": "12.0.2",
    "next-i18next": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.11.0",
    "react-relay": "^11.0.2",
    "react-relay-network-modern": "^6.0.0",
    "react-relay-network-modern-ssr": "^1.4.0",
    "reakit": "^1.3.8",
    "relay-hooks": "^4.2.0",
    "relay-runtime": "^11.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.0.9",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/react": "^6.3.12"
  }
}

{
  "name": "wdp-client",
  "workspaces": [
    "packages/admin",
    "packages/frontend",
    "packages/common/*"
  ],
  "version": "1.0.0",
  "description": "NGLP Web Delivery Platform client application",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "dev": "npm-run-all --parallel dev:*",
    "dev:frontend": "npm run dev --workspace=packages/frontend",
    "dev:admin": "npm run dev --workspace=packages/admin",
    "build": "npm-run-all --parallel build:*",
    "build:frontend": "npm run build --workspace=packages/frontend",
    "build:admin": "npm run build --workspace=packages/admin",
    "start": "npm-run-all --parallel start:*",
    "start:frontend": "npm run start --workspace=packages/frontend",
    "start:admin": "npm run start --workspace=packages/admin",
    "lint": "npm run lint --workspaces --if-present",
    "lint:js": "npm run lint --workspaces --if-present",
    "lint:css": "npm run lint --workspaces --if-present",
    "fix": "npm run fix --workspaces --if-present"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nglpteam/wdp-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nglpteam/wdp-client/issues"
  },
  "homepage": "https://github.com/nglpteam/wdp-client#readme",
  "dependencies": {
  },
  "devDependencies": {
    "aria-live-storybook-addon": "^0.0.6",
    "install": "^0.13.0",
    "npm": "^7.23.0",
    "npm-force-resolutions": "^0.0.10"
  },
  "resolutions": {
    "**/react": "^17.0.0"
  }
}


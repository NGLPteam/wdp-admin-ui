{
  "name": "wdp",
  "workspaces": [
    "packages/admin",
    "packages/frontend",
    "packages/lib",
    "packages/common/*"
  ],
  "cacheDirectories": [
    "node_modules",
    "packages/admin/node_modules",
    "packages/admin/.next/cache",
    "packages/frontend/node_modules",
    "packages/frontend/.next/cache",
    "packages/common/eslint-config/node_modules",
    "packages/common/stylelint-config/node_modules",
    "packages/lib/node_modules"
  ],
  "private": "true",
  "version": "1.0.0",
  "description": "NGLP Web Delivery Platform client application",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -l --parallel dev:*",
    "dev:frontend": "yarn workspace @wdp/frontend run dev",
    "dev:admin": "yarn workspace @wdp/admin run dev",
    "build": "npm-run-all -l --parallel build:*",
    "build:frontend": "yarn workspace @wdp/frontend run build",
    "build:admin": "yarn workspace @wdp/admin run build",
    "start": "npm-run-all -l --parallel start:*",
    "start:frontend": "yarn workspace @wdp/frontend run start",
    "start:admin": "yarn workspace @wdp/admin run start",
    "lint": "npm-run-all -l --parallel lint:js lint:css",
    "lint:js": "npm-run-all -l --parallel lint:js:*",
    "lint:js:frontend": "yarn workspace @wdp/frontend run lint:js",
    "lint:js:admin": "yarn workspace @wdp/admin run lint:js",
    "lint:css": "npm-run-all -l --parallel lint:css:*",
    "lint:css:frontend": "yarn workspace @wdp/frontend run lint:css",
    "lint:css:admin": "yarn workspace @wdp/admin run lint:css",
    "fix": "npm-run-all -l --parallel fix:*",
    "fix:frontend": "yarn workspace @wdp/frontend run fix",
    "fix:admin": "yarn workspace @wdp/admin run fix",
    "schema": "npm-run-all -l --parallel schema:*",
    "schema:frontend": "yarn workspace @wdp/frontend run schema",
    "schema:admin": "yarn workspace @wdp/admin run schema",
    "relay": "npm-run-all -l --parallel relay:*",
    "relay:frontend": "yarn workspace @wdp/frontend run relay",
    "relay:admin": "yarn workspace @wdp/admin run relay",
    "storybook": "npm-run-all -l --parallel storybook:run:*",
    "storybook:run:frontend": "yarn workspace @wdp/frontend run storybook",
    "storybook:run:admin": "yarn workspace @wdp/admin run storybook",
    "storybook:build": "npm-run-all -l --parallel storybook:build:*",
    "storybook:build:frontend": "yarn workspace @wdp/frontend run build-storybook",
    "storybook:build:admin": "yarn workspace @wdp/admin run build-storybook",
    "types:frontend": "yarn workspace @wdp/frontend run types",
    "types:admin": "yarn workspace @wdp/admin run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nglpteam/wdp-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nglpteam/wdp-client/issues"
  },
  "homepage": "https://github.com/nglpteam/wdp-client#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "install": "^0.13.0"
  },
  "engines": {
    "node": "14.x"
  }
}
